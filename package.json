    "react-pdf": "^7.7.1",
    "react-router-dom": "^6.14.1",
    "remark-gfm": "^4.0.0",
    "socket.io-client": "^4.7.4",
    "use-resize-observer": "^9.1.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
